  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros" />
  <PropertyGroup />
  <ItemDefinitionGroup>
    <PostBuildEvent>
      <Command>"$(ProjectDir)..\..\copy_assets\$(ProjectName)_copy_assets.bat"</Command>
